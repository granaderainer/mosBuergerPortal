@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using System.Security.Claims
@{
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Universal - All In 1 Template</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="robots" content="all,follow">
    <!-- Bootstrap CSS-->
    <link rel="stylesheet" href="~/user_template/vendor/bootstrap/css/bootstrap.min.css">
    <!-- Font Awesome CSS-->
    <link rel="stylesheet" href="~/user_template/vendor/font-awesome/css/font-awesome.min.css">
    <!-- Google fonts - Roboto-->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,700">
    <!-- Bootstrap Select-->
    <link rel="stylesheet" href="~/user_template/vendor/bootstrap-select/css/bootstrap-select.min.css">
    <!-- owl carousel-->
    <link rel="stylesheet" href="~/user_template/vendor/owl.carousel/assets/owl.carousel.css">
    <link rel="stylesheet" href="~/user_template/vendor/owl.carousel/assets/owl.theme.default.css">
    <!-- theme stylesheet-->
    <link rel="stylesheet" href="~/user_template/css/style.default.css" id="theme-stylesheet">
    <!-- Custom stylesheet - for your changes-->
    <link rel="stylesheet" href="~/user_template/css/custom.css">
    <!-- Favicon and apple touch icons-->
    <link rel="shortcut icon" href="~/user_template/img/favicon.ico" type="image/x-icon">
    <link rel="apple-touch-icon" href="~/user_template/img/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="57x57" href="~/user_template/img/apple-touch-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="72x72" href="~/user_template/img/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="~/user_template/img/apple-touch-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/user_template/img/apple-touch-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="~/user_template/img/apple-touch-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="~/user_template/img/apple-touch-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="~/user_template/img/apple-touch-icon-152x152.png">
    <!-- Tweaks for older IEs-->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script><![endif]-->
</head>
<body>
    <div id="all">
        <!-- Top bar-->
        @await Html.PartialAsync("_TopBarPartial")
        <!-- Top bar end-->
        <!-- Login modal end-->
        <!-- Navbar Start-->
        @await Html.PartialAsync("_NavBarView")
        <!-- Navbar End-->
        @RenderBody()
        <!-- Alles ab hier ist eig im RenderBody index hab ich nichts geändert -->
        <!--Hier hört Render Body auf-->
        <!-- FOOTER -->
        @await Html.PartialAsync("_FooterView")
        @await Html.PartialAsync("_Modal", new LoginViewModel())
    </div>
    <!-- Javascript files-->
    <script src="~/user_template/vendor/jquery/jquery.min.js"></script>
    <script src="~/user_template/vendor/popper.js/umd/popper.min.js"></script>
    <script src="~/user_template/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/user_template/vendor/jquery.cookie/jquery.cookie.js"></script>
    <script src="~/user_template/vendor/waypoints/lib/jquery.waypoints.min.js"></script>
    <script src="~/user_template/vendor/jquery.counterup/jquery.counterup.min.js"></script>
    <script src="~/user_template/vendor/owl.carousel/owl.carousel.min.js"></script>
    <script src="~/user_template/vendor/owl.carousel2.thumbs/owl.carousel2.thumbs.min.js"></script>
    <script src="~/user_template/js/jquery.parallax-1.1.3.js"></script>
    <script src="~/user_template/vendor/bootstrap-select/js/bootstrap-select.min.js"></script>
    <script src="~/user_template/vendor/jquery.scrollto/jquery.scrollTo.min.js"></script>
    <script src="~/user_template/js/front.js"></script>
    
</body>
</html>

